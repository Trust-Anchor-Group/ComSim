<?xml version="1.0" encoding="utf-8"?>
<Model xmlns="http://trustanchorgroup.com/Schema/ComSim.xsd"
       xmlns:xmpp="http://trustanchorgroup.com/Schema/ComSim/XMPP.xsd"
       timeBase="StartOfSimulation" 
       timeUnit="PT1S" 
       timeCycle="PT1M" 
       duration="PT1M">
