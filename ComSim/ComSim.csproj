<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Authors>Peter Waher</Authors>
    <Company>Trust Anchor Group AB</Company>
    <Description>The TAG Network Communication Simulator (or TAG ComSim) is a white-label console utility application written in C# provided by Trust Anchor Group (TAG for short). It can be used to simulate network communication traffic in large-scale networks.</Description>
    <Copyright>Copyright Â© Trust Anchor Group 2020. All rights reserved.</Copyright>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Schema\ComSim.xsd" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Schema\ComSim.xsd" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Waher.Content" Version="1.0.25" />
    <PackageReference Include="Waher.Content.Xml" Version="1.0.9" />
    <PackageReference Include="Waher.Content.Xsl" Version="1.0.4" />
    <PackageReference Include="Waher.Events" Version="1.0.12" />
    <PackageReference Include="Waher.Networking" Version="1.0.16" />
    <PackageReference Include="Waher.Persistence" Version="1.0.18" />
    <PackageReference Include="Waher.Persistence.Files" Version="1.0.71" />
    <PackageReference Include="Waher.Persistence.Serialization.Compiled" Version="1.0.3" />
    <PackageReference Include="Waher.Runtime.Inventory" Version="1.0.13" />
    <PackageReference Include="Waher.Runtime.Inventory.Loader" Version="1.0.8" />
    <PackageReference Include="Waher.Runtime.Timing" Version="1.0.2" />
    <PackageReference Include="Waher.Script" Version="1.0.39" />
    <PackageReference Include="Waher.Script.Content" Version="1.0.7" />
    <PackageReference Include="Waher.Script.Cryptography" Version="1.0.0" />
    <PackageReference Include="Waher.Script.Graphs" Version="1.0.15" />
    <PackageReference Include="Waher.Script.Persistence" Version="1.0.17" />
    <PackageReference Include="Waher.Script.Statistics" Version="1.0.3" />
    <PackageReference Include="Waher.Script.Xml" Version="1.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\TAG.Simulator\TAG.Simulator.csproj" />
  </ItemGroup>

</Project>
